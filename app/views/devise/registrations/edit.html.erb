<div class="form-actions edit-page">
    <h2 class="form-horizontal">Edit Account</h2>

    <%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => { :method => :put }) do |f| %>
      <%= f.error_notification %>

      <% if current_user.admin? %>
        <%= f.label :year %>
        <%= f.select :year, [['Freshman','freshman'],['Sophomore','sophomore'],['Junior','junior'], ['Senior', 'senior'], ['Leader', 'leader']] %><br><br>
        <%= f.label :school %>
        <%= f.select :school, [['UCI','uci'],['UCSB','ucsb'],['USC','usc'], ['UCLA', 'ucla'], ['UCSD', 'ucsd'], ['UCR', 'ucr'], ['Pepperdine', 'pepperdine'], ['CMU', 'cmu'], ['UCB', 'ucb'], ['Leader', 'leader'] ] %><br><br>
        <%= f.label :team %>
        <%= f.select :team, [['India','india'],['China','china'],['Japan','japan'], ['Nicaragua', 'nicaragua'], ['Cambodia', 'cambodia'], ['Philippines', 'philippines'], ['Turkey', 'turkey'], ['Leader', 'leader']] %><br><br>
      <% end %>

      <%= f.label :current_password %>
      <%= f.text_field :current_password %><br><br>
      <%= f.label :password %>
      <%= f.text_field :password, label: "New Password", autocomplete: "off" %><br><br>
      <%= f.label :password_confirmation %>
      <%= f.text_field :password_confirmation, label: "New Password Confirmation", autocomplete: "off" %><br><br>


        <%= f.submit "Update", class: "btn btn-primary btn-large" %>
    <% end %>

  </div>
